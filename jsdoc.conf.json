{
  "plugins": ["jsdoc-route-plugin"],
  "recurseDepth": 10,
  "source": {
    "include": ["src"],
    "exclude": ["src/workspaces", "node_modules"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "destination": "build/docs"
  }
}
